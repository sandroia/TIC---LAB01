#include<stdio.h>

int main()
{
    int sal, novsal; 
    printf("Digite o valor do sal치rio:");
    scanf("%i", &sal);
    printf ("Digite o valor do novo sal치rio:");
    scanf("%i", &novsal);
    novsal = sal + (sal * 25/100);
    printf ("\n O novo sal치rio do funcion치rio e: %i\n", novsal);
    return 0;
}